{
  "name": "roro-kube-web-docs",
  "version": "0.1.0",
  "description": "Documentation site for Roro Kube",
  "type": "module",
  "dependencies": {
    "@mdx-js/react": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "gray-matter": "^4.0.3",
    "prettier": "^3.1.0",
    "remark-frontmatter": "^5.0.0",
    "vite": "^5.0.8"
  },
  "scripts": {
    "generate": "node scripts/generate-pages.mjs",
    "build": "npm run generate && vite build && BASE_PATH=${BASE_PATH:-/} node scripts/prerender.mjs",
    "dev": "npm run generate && vite dev",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mjs}\" \"scripts/**/*.{js,mjs}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mjs}\" \"scripts/**/*.{js,mjs}\""
  }
}

